{
  "branches": ["main"],
  "tagFormat": "${version}",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/github",
    [
      "semantic-release-cocoapods",
      {
        "podLintArgs": "TokenTextView.podspec",
        "podPushArgs": "TokenTextView.podspec"
      }
    ]
  ]
}
